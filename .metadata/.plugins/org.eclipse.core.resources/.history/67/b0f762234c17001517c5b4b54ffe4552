package ba.bitcamp.homework_stringbuilder_task2;

public class Author {
	private String name;
	private String email;
	private boolean sex;
	
	/**
	 * Constructor
	 * @param name
	 * @param email
	 * @param sex
	 */
	public Author(String name, String email, boolean sex) {
		super();
		this.name = name;
		this.email = email;
		this.sex = sex;
	}
	
	
}
