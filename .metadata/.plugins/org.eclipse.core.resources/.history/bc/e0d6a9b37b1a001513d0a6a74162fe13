package homework_oop1_task2;

import homework_oop1_task1.Computer;

public abstract class Network {
	private String name;
	private Computer[] computers;
	
	/**
	 * @param name
	 * @param computers
	 */
	public Network(String name, Computer[] computers) {
		super();
		this.name = name;
		this.computers = computers;
	}

	/**
	 * @param name
	 */
	public Network(String name) {
		super();
		this.name = name;
		this.computers = new Computer[0];
	}
	
	public abstract void addComputer(Computer c);
	
	public abstract void removeComputer(Computer c);
	
	 
}
