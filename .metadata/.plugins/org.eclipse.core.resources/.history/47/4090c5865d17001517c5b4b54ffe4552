package ba.bitcamp.homework_stringbuilder.task3;

public class Date {
	private int day;
	private int month = 1;
	private int year = 1;
	
	/**
	 * @param day
	 * @param month
	 * @param year
	 */
	public Date(int day, int month, int year) {
		super();
		this.day = day;
		this.month = month;
		this.year = year;
	}
	
	
	
	
}
